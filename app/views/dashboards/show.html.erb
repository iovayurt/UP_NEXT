<h1>My Orders</h1>
<div class="tab-content" id="myTabContent">
  <div class="container">
    <section class="wrapper">

    </section>
  </div>
</div>


<% @orders.each do |order| %>
  <div class="card-trip">
    <img src= "<%= order.restaurant.photo_url %>" />
    <div class="card-trip-infos">
      <div>

            <%= render "shared/order_card", order: order %>

      </div>
      <h2 class="card-trip-pricing"></h2>
    </div>
  </div>
<% end %>
